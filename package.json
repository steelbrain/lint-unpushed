{
  "name": "lint-unpushed",
  "version": "0.3.0",
  "description": "Lint code before it's pushed",
  "bin": "lib/cli.js",
  "scripts": {
    "lint": "(tsc -p . --noEmit) && (eslint . --ext .ts) && (prettier --list-different src/*.ts)",
    "prepare": "sb-babel-cli src -o lib --typescript",
    "watch": "yarn prepare -watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steelbrain/lint-unpushed.git"
  },
  "author": "steelbrain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/steelbrain/lint-unpushed/issues"
  },
  "files": [
    "lib/*"
  ],
  "homepage": "https://github.com/steelbrain/lint-unpushed#readme",
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/listr": "^0.14.2",
    "@types/micromatch": "^4.0.1",
    "@types/node": "^14.0.27",
    "@types/shell-escape": "^0.2.0",
    "eslint-config-steelbrain": "^10.0.0-beta2",
    "sb-babel-cli": "^3.0.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@steelbrain/spawn": "^0.2.0",
    "commander": "^6.0.0",
    "listr": "^0.14.3",
    "micromatch": "^4.0.2",
    "shell-escape": "^0.2.0",
    "zen-observable": "^0.8.15"
  },
  "engines": {
    "node": ">= 8"
  }
}
